<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itfhc.babytun.dao.goodsDetailDao">
    <select id="findDetailById" parameterType="Long" resultType="com.itfhc.babytun.entity.goodsDetail">
        select * from t_goods_detail where goods_id = #{Id};
